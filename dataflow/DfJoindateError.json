{
	"name": "DfJoindateError",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DSJoindateError",
						"type": "DatasetReference"
					},
					"name": "Src"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "Conditionalsplit1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmpID as string,",
				"          Name as string,",
				"          Country as string,",
				"          DeptName as string,",
				"          JoinDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> Src",
				"Src split(isNull(toDate(JoinDate,'dd-mm-yyyy')),",
				"     disjoint: false,",
				"     partitionBy('hash', 1)) ~> Conditionalsplit1@(Errorowa, Goodrows)"
			]
		}
	}
}