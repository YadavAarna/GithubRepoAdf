{
	"name": "DFremovedups",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText4distinct",
						"type": "DatasetReference"
					},
					"name": "Dupsrc"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmpID as integer,",
				"          Name as string,",
				"          Country as string,",
				"          DeptName as string,",
				"          JoinDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> Dupsrc",
				"Dupsrc aggregate(groupBy(EmpID),",
				"     each(match(name!='EmpID'), $$ = ERROR_FUNCTION(''))) ~> aggregate1"
			]
		}
	}
}